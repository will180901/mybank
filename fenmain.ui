/*===============================================
   COMBOBOX
   ===============================================*/

QComboBox {
    background-color: rgb(255, 255, 255);
    border: 1px solid #ced4da;
    border-radius: 4px;
    padding: 6px 24px 6px 8px;
    color: #495057;
    font-size: 13px;
    min-height: 18px;
    selection-background-color: #0d6efd;
}

QComboBox:hover {
    border-color: #0d6efd;
    background-color: rgb(248, 249, 250);
}

QComboBox:focus {
    border: 1px solid #0d6efd;
    outline: none;
}

QComboBox:pressed {
    background-color: rgb(240, 244, 248);
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border: none;
    background-color: transparent;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
}

QComboBox::drop-down:hover {
    background-color: rgba(229, 234, 239, 0.3);
}

QComboBox::drop-down:pressed {
    background-color: rgba(229, 234, 239, 0.5);
}

QComboBox::down-arrow {
    image: url(:/icon_gris/chevron-down.svg);
    width: 12px;
    height: 12px;
}

QComboBox::down-arrow:pressed {
    image: url(:/icon_noir/chevron-down.svg);
}

QComboBox QAbstractItemView {
    border: 1px solid #0d6efd;
    border-radius: 4px;
    background-color: rgb(255, 255, 255);
    selection-background-color: #0d6efd;
    selection-color: white;
    padding: 2px;
    outline: none;
    show-decoration-selected: 1;
}

QComboBox QAbstractItemView::item {
    height: 28px;
    padding: 0 8px;
    border-bottom: 1px solid rgba(229, 234, 239, 0.5);
    color: #495057;
}

QComboBox QAbstractItemView::item:last {
    border-bottom: none;
}

QComboBox QAbstractItemView::item:hover {
    background-color: rgba(13, 110, 253, 0.1);
    color: #0d6efd;
}

QComboBox QAbstractItemView::item:selected {
    background-color: #0d6efd;
    color: white;
}

